                <box size="0.1 0.1 0.1"/>
            </geometry>

            <material name="blue">
                <color rgba="0 0 .8 1"/>
            </material>
        </visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <collision>
             <geometry>
                   <cylinder length="0.1" radius="0.25"/>
              </geometry>
             <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        </collision>
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="1" />
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
        </inertial>
    </link>
    <link name="car_frame">
